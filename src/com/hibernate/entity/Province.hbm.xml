<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="com.hibernate.entity.Province" table="PROVINCE">
        <id name="provinceNumber" type="java.lang.String" column="PROVINCE_NUMBER">
            <generator class="increment"/>
        </id>
        <property name="provinceName" type="java.lang.String" column="PROVINCE_NAME" length="40"/>
        <property name="provinceASR" type="java.lang.String" column="PROVINCE_ASR" length="40"/>
    </class>
</hibernate-mapping>